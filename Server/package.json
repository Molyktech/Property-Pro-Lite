{
  "name": "property-pro-lite",
  "version": "1.0.0",
  "description": "A Node.js restful api and project back end",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel ./src --out-dir ./build -s  --copy-files",
    "build": "npm run clean && npm run build-server",
    "start": "set NODE_ENV='PROD' && node ./build/server.js",
    "test": "nyc mocha ./src/test/*.js --recursive --require @babel/register --require @babel/polyfill --timeout=6000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "nodemon --exec @babel/node ./src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Molyktech/Property-Pro-Lite.git"
  },
  "author": "\"Modupe Adebayo\"",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Molyktech/Property-Pro-Lite/issues"
  },
  "homepage": "https://github.com/Molyktech/Property-Pro-Lite#readme",
  "dependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.0.0",
    "@hapi/joi": "^15.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.14.0",
    "datauri": "^2.0.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.1"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2"
  }
}